clear; clc; close all;

% Defining time data
time = linspace(0, 22, 22);

% Defining symbolic variable
syms t

% Defining piecewise function
v = piecewise(t < 2, 0, 2 < t < 4.5, (200 * t) - 400, 4.5 < t < 7, 500, ...
    7 < t < 9, (250 * t) - 900, 9 < t < 14, 1000, 14 < t < 22, ...
    (-200 * t) + 3800);

fplot(v)
xlim([0, 22])

